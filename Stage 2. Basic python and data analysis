    "p3=0\n",
    "\n",
    "\n",
    "\n",
    "for i in range(len(segment)):\n",
    "    if segment[i]== 'Segment 0':\n",
    "        t1 +=robocats[i]\n",
    "        quontity_1+=1\n",
    "    elif  segment[i]== 'Segment 1':\n",
    "        t2 +=robocats[i]\n",
    "        quontity_2+=1\n",
    "    elif segment[i]== 'Segment 2':\n",
    "        t3 +=robocats[i] \n",
    "        quontity_3+=1\n",
    "p1= (t1/quontity_1)\n",
    "p2= (t2/quontity_2)\n",
    "p3= (t3/quontity_3)\n",
    "\n",
